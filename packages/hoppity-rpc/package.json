{
    "name": "@apogeelabs/hoppity-rpc",
    "version": "0.1.0",
    "description": "RPC utilities for hoppity",
    "author": "Apogee Travel",
    "contributors": [
        {
            "name": "Jim Cowart"
        }
    ],
    "license": "ISC",
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "engines": {
        "node": ">=18"
    },
    "sideEffects": false,
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "test": "jest",
        "test:watch": "jest --watch --collect-coverage",
        "lint": "eslint --config ../../.eslintrc.js 'src/**/*.{ts,tsx}'",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "keywords": [
        "hoppity",
        "rabbitmq",
        "rascal",
        "amqp",
        "rpc"
    ],
    "devDependencies": {
        "@types/jest": "^29.5.5",
        "@types/lodash": "^4.14.202",
        "@types/rascal": "^10.2.1",
        "eslint": "^8.0.1",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
        "eslint-plugin-promise": "^6.0.0",
        "jest": "^29.7.0",
        "rascal": "^20.1.1",
        "ts-jest": "^29.1.1",
        "tsup": "^7.2.0",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "rascal": "^20.1.1",
        "@apogeelabs/hoppity": "workspace:*",
        "lodash": "^4.17.21"
    }
}
